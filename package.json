{
  "name": "happydogs",
  "description": "Angular TS Frontend",
  "license": "MIT",
  "version": "0.2.1-0",
  "author": "Dominik Dorfstetter",
  "private": true,
  "engines": {
    "node": "^12.14.1",
    "npm": "^6.13.4",
    "yarn": "^1.21.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "yarn run inject-version && ng serve --proxy-config proxy.conf.json",
    "#### MOCK ####": "################################################",
    "mock:server": "json-server --watch mocks/api.json",
    "start:mock": "yarn run inject-version && ng serve --proxy-config proxy-mock.conf.json",
    "#### BUILD ####": "###############################################",
    "build": "yarn run inject-version && ng build --prod",
    "build:dev": "yarn run inject-version:dev && ng build --prod --configuration=dev",
    "build:prod": "yarn run inject-version:prod && ng build --prod --configuration=production",
    "#### TEST ####": "################################################",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "#### LINT & PRETTIER ####": "#####################################",
    "pretty": "pretty-quick",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "### VERSION NUMBER ###": "#####################################",
    "bump-minor": "gulp ng-bump-minor",
    "bump-major": "gulp ng-bump-major",
    "bump-prerelease": "gulp ng-bump-prerelease",
    "inject-version": "gulp ng-inject-version",
    "inject-version:dev": "gulp ng-inject-version --configuration=dev",
    "inject-version:prod": "gulp ng-inject-version --configuration=prod"
  },
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@cds/core": "^5.0.3",
    "@clr/angular": "^5.0.3",
    "@clr/core": "^4.0.8",
    "@clr/icons": "^5.0.3",
    "@clr/ui": "^5.0.3",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "leaflet": "^1.6.0",
    "ng-bullet": "^1.0.3",
    "rxjs": "~6.6.3",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.3",
    "xregexp": "^4.4.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular-eslint/builder": "2.0.1",
    "@angular-eslint/eslint-plugin": "2.0.1",
    "@angular-eslint/eslint-plugin-template": "2.0.1",
    "@angular-eslint/schematics": "2.0.1",
    "@angular-eslint/template-parser": "2.0.1",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "codelyzer": "^6.0.0",
    "del": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "latest",
    "eslint-plugin-sonarjs": "^0.6.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-replace": "^1.0.0",
    "husky": "^4.3.6",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "json-server": "^0.16.3",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "<4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && git add .",
      "pre-push": "npm run lint && npm run test && npm run build"
    }
  }
}
