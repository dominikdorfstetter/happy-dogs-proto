{
  "name": "happydogs",
  "description": "Angular TS Frontend",
  "license": "MIT",
  "version": "0.2.1-0",
  "author": "Dominik Dorfstetter",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "yarn run inject-version && ng serve --proxy-config proxy.conf.json",
    "#### MOCK ####": "################################################",
    "mock:server": "json-server --watch mocks/api.json",
    "start:mock": "yarn run inject-version && ng serve --proxy-config proxy-mock.conf.json",
    "#### BUILD ####": "###############################################",
    "build": "yarn run inject-version && ng build --configuration production",
    "build:dev": "yarn run inject-version:dev && ng build --prod --configuration dev",
    "build:prod": "yarn run inject-version:prod && ng build --prod --configuration production",
    "#### TEST ####": "################################################",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "#### LINT & PRETTIER ####": "#####################################",
    "pretty": "pretty-quick",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "### VERSION NUMBER ###": "#####################################",
    "bump:major": "gulp ng-bump-major",
    "bump:minor": "gulp ng-bump-minor",
    "bump:patch": "gulp ng-bump-patch",
    "bump:prerelease": "gulp ng-bump-prerelease",
    "inject-version": "gulp ng-inject-version",
    "inject-version:dev": "gulp ng-inject-version --configuration=dev",
    "inject-version:prod": "gulp ng-inject-version --configuration=prod",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "^12.0.0-rc.1",
    "@angular/common": "^12.0.0-rc.1",
    "@angular/compiler": "^12.0.0-rc.1",
    "@angular/core": "^12.0.0-rc.1",
    "@angular/forms": "^12.0.0-rc.1",
    "@angular/platform-browser": "^12.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^12.0.0-rc.1",
    "@angular/router": "^12.0.0-rc.1",
    "@cds/core": "^5.0.3",
    "@clr/angular": "^5.0.3",
    "@clr/core": "^4.0.12",
    "@clr/icons": "^5.0.3",
    "@clr/ui": "^5.0.3",
    "@openapitools/openapi-generator-cli": "^2.2.6",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "baconjs": "^3.0.17",
    "classlist.js": "^1.1.20150312",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.6.0",
    "rxjs": "^6.5.3",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0-rc.1",
    "@angular-eslint/builder": "4.2.0",
    "@angular-eslint/eslint-plugin": "4.2.0",
    "@angular-eslint/eslint-plugin-template": "4.2.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "4.2.0",
    "@angular/cli": "^12.0.0-rc.1",
    "@angular/compiler-cli": "^12.0.0-rc.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "codelyzer": "^6.0.0",
    "del": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "33.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-react": "latest",
    "eslint-plugin-sonarjs": "^0.7.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-replace": "^1.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "json-server": "^0.16.3",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "<4.3.0"
  }
}
